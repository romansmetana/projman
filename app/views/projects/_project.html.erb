<ul id="<%= "#{dom_id(project)}_item" %>">
  <%= turbo_frame_tag dom_id(project) do %>
    <div class="d-flex justify-content-between">
      <%= link_to project.title, edit_project_path(project) %>
      <%= button_to t("helpers.submit.project.delete"), project_path(project), method: :delete %>
      <div id="<%= "#{dom_id(project)}_block" %>">
        <%= render project.tasks %>
      </div>
    </div>
  <% end %>
</ul>
